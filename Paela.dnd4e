            <specific name="_SupportsID" > ID_FMP_CLASS_5 </specific>
         </RulesElement>
         <RulesElement name="Twin Strike" type="Power" internal-id="ID_FMP_POWER_87" url="http://www.wizards.com/dndinsider/compendium/power.aspx?id=87" charelem="142" legality="rules-legal" >
            <specific name="Flavor" > If the first attack doesn&apos;t kill it, the second one might. </specific>
            <specific name="Power Usage" > At-Will </specific>
            <specific name="Display" > Ranger Attack 1 </specific>
            <specific name="Keywords" > Martial, Weapon </specific>
            <specific name="Action Type" > Standard Action </specific>
            <specific name="Attack Type" > Melee or Ranged weapon </specific>
            <specific name="Requirement" > You must be wielding two melee weapons or a ranged weapon. </specific>
            <specific name="Targets" > One or two creatures </specific>
            <specific name="Attack" > Strength vs. AC (melee; main weapon and off-hand weapon) or Dexterity vs. AC (ranged), two attacks </specific>
